# Configuration file for astro_sws_alerts
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file with real credentials!

# Your phone number (without carrier domain)
PHONE_NUMBER=5551234567

# Gmail account for sending SMS alerts
EMAIL=your-email@gmail.com

# Gmail app password (not your regular password)
# See: https://support.google.com/accounts/answer/185833
PASSWORD=your-app-password

# URL to your SWS web interface
SWS_URL=http://your-sws-url/index.htm
